<?xml version="1.0"?>
<launch>

  <!-- Run the frame kinematics node -->
  <node pkg="artificial_hands_base" type="frame_kinematics_node" name="frame_kinematics_node" output="screen" args="--autostart true"/>

  <!-- rqt plot -->
  <node pkg="rqt_gui" type="rqt_gui" name="rqt_plot_frame_kinematics" output="screen" args="--perspective-file $(find artificial_hands_bringup)/etc/frame_kinematics.perspective"/>

</launch>